<?xml version="1.0" encoding="utf-8"?>
<RunSettings>
	<DataCollectionRunSettings></DataCollectionRunSettings>
	<TestRunParameters>
		<!-- General Configuration -->
		<Parameter name="ENV" value="#{ENV}#" />
		<Parameter name="BaseURL" value="#{BaseURL}#" />
		<Parameter name="BaseurlAPI" value="#{BaseurlAPI}#" />
		<Parameter name="DefaultTimeoutSeconds" value="#{DefaultTimeoutSeconds}#" />
		<Parameter name="WaitTimeoutInMinutes" value="#{WaitTimeoutInMinutes}#" />

		<!-- User Credentials for ROPC Flow -->
		<Parameter name="UserName" value="#{UserNameAzure}#" />
		<Parameter name="UserPassword" value="#{AzureNonProdUserPassword}#" />

		<!-- Azure AD Configuration -->
		<Parameter name="EntraIdTenantId" value="#{EntraIdTenantId}#" />
		<Parameter name="EntraIdClientId" value="#{EntraIdClientId}#" />
		<Parameter name="EntraIdClientSecret" value="#{EntraIdClientSecret}#" />
		<Parameter name="EntraIdApiScope" value="#{EntraIdApiScope}#" />
		<Parameter name="EntraIdAuthority" value="https://login.microsoftonline.com" />

		<!-- Performance Configuration -->
		<Parameter name="UseSmartWaitStrategies" value="true" />
		<Parameter name="WaitForAjaxCompletion" value="true" />
		<Parameter name="WaitForPageLoadState" value="true" />
		<Parameter name="ElementVisibilityTimeoutSeconds" value="15" />
		<Parameter name="ElementClickabilityTimeoutSeconds" value="10" />
		<Parameter name="PageTransitionTimeoutSeconds" value="10" />
		<Parameter name="RetryDelayMilliseconds" value="500" />
		<Parameter name="MaxRetryAttempts" value="3" />

		<!-- Logging Configuration -->
		<Parameter name="EnableStructuredLogging" value="true" />
		<Parameter name="LogLevel" value="Information" />
		<Parameter name="MaskSensitiveDataInLogs" value="true" />

		<!-- Security Configuration -->
		<Parameter name="ForceHttps" value="false" />
		<Parameter name="EnableSecureTokenStorage" value="false" />
		<Parameter name="EnableAuditLogging" value="false" />

		<!-- Email for Test Results -->
		<Parameter name="EMailTo" value="#{EMailTo}#" />
	</TestRunParameters>
	<MSTest>
	</MSTest>
</RunSettings>
